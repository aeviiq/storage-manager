{
    "name": "aeviiq/storage-manager",
    "type": "library",
    "description": "Storage Manager Component",
    "keywords": [
        "storage",
        "storage-manager"
    ],
    "homepage": "https://github.com/aeviiq/storage-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Aeviiq Storage Manager contributors",
            "homepage": "https://github.com/aeviiq/storage-manager/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aeviiq\\StorageManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aeviiq\\Tests\\StorageManager\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "symfony/http-foundation": "^6.0",
        "myclabs/deep-copy": "^1.11",
        "doctrine/persistence": "^2.1|^3.0",
        "doctrine/common": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.27",
        "phpstan/phpstan": "^1.8",
        "psalm/plugin-phpunit": "^0.17.0",
        "thecodingmachine/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.4"
    }
}
